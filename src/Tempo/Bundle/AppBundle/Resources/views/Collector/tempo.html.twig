{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {# Tempo Logo #}
    {% set icon %}
    <a href="http://tempo-project.org/">
        <img alt="" src="data:image/png;base64," />        <span>{{ collector.version }}</span>
    </a>
    {% endset %}
    {% set text %}
    <div class="sf-toolbar-info-piece">
        Tempo <b>{{ collector.version }}</b>
    </div>
    <div class="sf-toolbar-info-piece">
        <a href="https://www.tempo-project.com" rel="help">Tempo Documentation</a>
    </div>
    {% endset %}
    {% include '@WebProfiler/Profiler/toolbar_item.html.twig' with {'link': false} %}
{% endblock %}
