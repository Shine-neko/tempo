{% set data = event.getData('json_decode') %}

<div class="event">
    <time  datetime="2013-11-14T09:45:16Z" title="2013-11-14 09:45:16">17 minutes ago</time>
</div>
<div class="action">
    <ul>
        <li>
           {{ data.pusher.name }} pushed to  {{ data.repository.master_branch }}  {{ data.repository.name  }}/{{ data.repository.organization }}
           {% if data.pusher.name != 'none' %}
           <div class="details">
               <img height="16" src="{{ gravatar(data.pusher.name.email, 30) }}">
           </div>
           {% endif %}
           <ul class="commits">
               {% for commit in event.getData('json_decode').commits %}
                   <li>
                       <span> <img height="16" src="{{ gravatar(commit.author.email, 16) }}"> </span>
                       <a href="{{ commit.url }}"> {{ truncate(commit.id, 7) }} </a> {{ commit.message }}
                   </li>
               {% endfor %}
           </ul>
        </li>

        <a class="more" href="{{ data.compare }}">View comparison for these {{ data.commits|length }} commits Â»</a>
    </ul>
</div>