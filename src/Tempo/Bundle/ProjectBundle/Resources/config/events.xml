<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>

        <parameter key="tempo.event.listener.team.class">Tempo\Bundle\ProjectBundle\EventListener\TeamSubscriber</parameter>

    </parameters>

    <services>

        <service id="tempo.event.listener.team" class="%tempo.event.listener.team.class%">

            <argument type="service" id="translator" />
            <argument type="service" id="router" />
            <argument type="service" id="tempo.manager.notification" />
            <tag name="kernel.event_subscriber" />
        </service>

    </services>

</container>