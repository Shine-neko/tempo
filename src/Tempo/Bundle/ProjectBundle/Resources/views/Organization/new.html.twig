{% extends app.request.isXmlHttpRequest ? "TempoMainBundle::Layout/ajax.html.twig" : "TempoMainBundle::Layout/web.html.twig" %}
{% block javascripts %}
{{ parent() }}
    <script type="text/javascript" src="{{ asset('bundles/tempoproject/js/views/organizationView.js')}}"></script>
{% endblock %}

{% block content %}
    <form action="{{ path('organization_create') }}" method="post" class="organization-add clearfix">

        {{ form_widget(form) }}

        <div class="rest">
            <input type="submit" class="submit btn btn-primary" value="{{ 'tempo.form.save'|trans }}" />
        </div>
    </form>
{% endblock %}
