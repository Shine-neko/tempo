{% extends app.request.isXmlHttpRequest ? "TempoMainBundle::Layout/ajax.html.twig" : "TempoMainBundle::Layout/web.html.twig" %}
{% block javascripts %}
{{ parent() }}
<script type="text/javascript" src="{{ asset('bundles/tempoproject/js/client.js')}}"></script>
{% endblock %}


{% block content %}
    <form action="{{ path('client_create') }}" method="post" {{ form_enctype(form) }} class="client-add clearfix">

        {{ form_widget(form) }}

        <div class="rest">
            <input type="submit" class="submit btn" />
        </div>
    </form>
{% endblock %}



{% block right %}
    <div title="Upgrade" class="add_new_btn">
        <a href="#">Add<span></span></a>
    </div>
    <div id="clients">
        <ul>

        </ul>
    </div>
{% endblock %}
