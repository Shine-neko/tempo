<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="tempo.main.eventlistener.class">Tempo\Bundle\MainBundle\EventListener\ControllerListener</parameter>
        <parameter key="tempo.main.elephantio.client.class">Tempo\Bundle\MainBundle\ElephantIO\Client</parameter>
        <parameter key="tempo.data_collector.class">Tempo\Bundle\MainBundle\DataCollector\TempoDataCollector</parameter>
        <parameter key="tempo.form.type.autocomplete.class">Tempo\Bundle\MainBundle\Form\Type\AutocompleteType</parameter>
        <parameter key="tempo.form.type.datetimepicker.class">Tempo\Bundle\MainBundle\Form\Type\DateTimePickerType</parameter>
        <parameter key="tempo.form.type.list.class">Tempo\Bundle\MainBundle\Form\Type\ListType</parameter>
        <parameter key="tempo.main.menu_builder.class">Tempo\Bundle\MainBundle\Menu\MenuBuilder</parameter>
        <parameter key="tempo.main.behavior_manager.class">Tempo\Bundle\MainBundle\Helper\Behavior</parameter>
        <parameter key="tempo.main.resource_manager.class">Tempo\Component\Resource\ResourceManager</parameter>
        <parameter key="tempo.main.menu.class">Knp\Menu\MenuItem</parameter>
        <parameter key="tempo.model.room.class">Tempo\Bundle\MainBundle\Entity\Room</parameter>
        <parameter key="tempo.model.room.message.class">Tempo\Bundle\MainBundle\Entity\ChatMessage</parameter>
        <parameter key="tempo.manager.team.class">Tempo\Bundle\MainBundle\Manager\TeamManager</parameter>
        <parameter key="tempo.manager.room.class">Tempo\Bundle\MainBundle\Manager\RoomManager</parameter>
        <parameter key="tempo.manager.room.message.class">Tempo\Bundle\MainBundle\Manager\RoomMessageManager</parameter>
        <parameter key="tempo.main.listener.project.class">Tempo\Bundle\MainBundle\EventListener\ProjectListener</parameter>
        <parameter key="tempo.main.settings_schema.generale.class">Tempo\Bundle\MainBundle\Settings\GeneralSettingsSchema</parameter>
        <parameter key="tempo.main.settings_schema.project.class">Tempo\Bundle\MainBundle\Settings\ProjectSettingsSchema</parameter>
        <parameter key="tempo.main.twig.extension.class">Tempo\Bundle\MainBundle\Twig\Extension\MainExtension</parameter>
        <parameter key="tempo.manager.activity.class">Tempo\Bundle\MainBundle\Manager\ActivityManager</parameter>
        <parameter key="tempo.model.activity.class">Tempo\Bundle\MainBundle\Entity\Activity</parameter>
    </parameters>

    <services>

        <service id="twig.extension.intl" class="Twig_Extensions_Extension_Intl">
            <tag name="twig.extension" />
        </service>

        <service id="tempo.main.entity_manager" factory-service="doctrine" factory-method="getManager" class="Doctrine\ORM\EntityManager" public="false" />

        <service id="tempo.main.listener.controller" class="%tempo.main.eventlistener.class%">
            <tag name="kernel.event_listener" event="kernel.controller" method="onKernelController"/>
        </service>

        <service id="tempo.main.elephantio.definition" class="ElephantIO\Client" public="false" >
            <argument type="string">http://%socket_io.client%</argument>
        </service>

        <service id="tempo.main.elephantio.client" class="%tempo.main.elephantio.client.class%">
            <argument type="service" id="tempo.main.elephantio.definition" />
        </service>

        <service id="tempo.form.type.autocomplete" class="%tempo.form.type.autocomplete.class%">
            <argument type="service" id="tempo.main.behavior" />
            <tag name="form.type" alias="autocomplete" />
        </service>

        <service id="tempo.form.type.datetimepicker" class="%tempo.form.type.datetimepicker.class%">
            <tag name="form.type" alias="datetimepicker" />
        </service>

        <service id="tempo.form.type.list" class="%tempo.form.type.list.class%">
            <tag name="form.type" alias="list" />
        </service>

        <service id="tempo.data_collector" class="%tempo.data_collector.class%" public="false">
            <tag name="data_collector" template="TempoMainBundle:Collector:tempo" id="tempo" priority="256" />
        </service>

        <service id="tempo.main.resource_manager" class="%tempo.main.resource_manager.class%" />

        <service id="tempo.main.behavior" class="%tempo.main.behavior_manager.class%">
            <argument type="service" id="tempo.main.resource_manager" />
            <argument type="service" id="router" />
        </service>

        <service id="tempo.main.twig.extension" class="%tempo.main.twig.extension.class%">
            <argument type="service" id="service_container" />
            <argument type="service" id="time.templating.helper.time" />
            <tag name="twig.extension" />
        </service>

        <service id="tempo.main.menu_builder" class="%tempo.main.menu_builder.class%">
            <argument type="service" id="knp_menu.factory" />
            <argument type="service" id="security.context" />
            <argument type="service" id="translator" />
        </service>

        <service id="tempo.main.menu" class="%tempo.main.menu.class%" factory-service="tempo.main.menu_builder" factory-method="mainMenu">
            <tag name="knp_menu.menu" alias="main" />
        </service>

        <service id="tempo.main.breadcrumb" class="%tempo.main.menu.class%" factory-service="tempo.main.menu_builder" factory-method="breadcrumb">
            <tag name="knp_menu.menu" alias="breadcrumb" />
        </service>

        <service id="tempo.main.settings_schema.generale" class="%tempo.main.settings_schema.generale.class%">
            <tag name="sylius.settings_schema" namespace="general" />
        </service>

        <service id="tempo.main.settings_schema.project" class="%tempo.main.settings_schema.project.class%">
            <tag name="sylius.settings_schema" namespace="project" />
        </service>

        <service id="tempo.manager.team" class="%tempo.manager.team.class%">
            <argument type="service" id="doctrine.orm.entity_manager" />
        </service>

        <service id="tempo.manager.room" class="%tempo.manager.room.class%">
            <argument type="service" id="doctrine.orm.entity_manager" />
            <argument>%tempo.model.room.class%</argument>
        </service>

        <service id="tempo.manager.room.message" class="%tempo.manager.room.message.class%">
            <argument type="service" id="doctrine.orm.entity_manager" />
            <argument>%tempo.model.room.message.class%</argument>
        </service>

        <service id="tempo.main.listener.project" class="%tempo.main.listener.project.class%">
            <argument type="service" id="tempo.manager.room" />
            <tag name="kernel.event_listener" event="tempo.project.create.success" method="createProject" />
        </service>

    </services>

</container>